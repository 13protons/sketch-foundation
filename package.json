{
  "name": "sketch-foundation",
  "description": "Sketch Plugin to turn named layers on/off on all document artboards as a way to create foundational layers like notes and guides more manageable",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "sketch-foundation",
    "manifest": "src/manifest.json",
    "main": "sketch-foundation.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Alan Languirand <alanlanguirand@quickenloans.com>",
  "repository": "https://github.com/13protons/sketch-foundation",
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
